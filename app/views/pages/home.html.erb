<div class="card-container">
  <% @diaries.each do |diary| %>
    <div class="card">
      <div class="card-header">
        <div class="diary-name"><%= diary.title %></div>
      </div>
      <div class="card-body">
        <button class="button new-entry">New Entry</button>
        <button class="button settings">Settings</button>
      </div>
    </div>
  <% end %>
  <button><%= link_to "New Diary", new_diary_path %></button>
</div>
